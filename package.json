{
  "name": "all-in-one-swipper",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite --mode development",
    "electron:build": "vite build && electron-builder",
    "electron:preview": "electron ."
  },
  "browser": {
    "html2pdf.js": "html2pdf.js/dist/html2pdf.min.js"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^32.3.3",
    "@mdi/font": "^7.4.47",
    "@primevue/themes": "^4.2.5",
    "@tfoxy/v-tables-3": "^0.4.9",
    "@vuepic/vue-datepicker": "^10.0.0",
    "ag-grid-vue3": "^33.0.3",
    "animate.css": "^4.1.1",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "chartjs-plugin-annotation": "^3.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "html2pdf": "^0.0.11",
    "html2pdf.js": "^0.10.2",
    "jspdf": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "3.15.0",
    "v-tables-3": "^0.4.7",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-html2pdf": "^1.8.0",
    "vue-pagination-2": "^3.1.0",
    "vue-router": "4",
    "vue-tables-3": "^2.0.1-beta05",
    "vue3-html2pdf": "^1.1.2",
    "vue3-to-pdf": "^1.0.8",
    "vuetify": "^3.0.0",
    "vuex": "^4.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "sass-embedded": "^1.85.0",
    "vite": "^6.0.5",
    "vite-plugin-electron": "^0.29.0"
  },
  "build": {
    "appId": "com.all-in-one-swiper.id",
    "productName": "ALL IN ONE SWIPER",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./Icons/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
